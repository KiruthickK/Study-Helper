<%- include('Header'); -%>

    <body>
        <div class="container bg-light pt-3">
            <h1>Hello</h1>
            <h1>List of Courses</h1>

            <ul>
                <% courses.forEach(course=> { %>
                    <li>
                        <strong>Course ID:</strong>
                        <%= course.C_Id %>,
                            <strong>Course Name:</strong>
                            <%= course.Course_name %>
                    </li>
                    <% }); %>
            </ul>

        </div>
        <script src="/scripts/coursePage.js"></script>
    </body>

    </html>