    <div class="container bg-light pt-3 pb-2 mt-3 rounded">
        <!-- sdfgs -->

        <!-- rgerg -->
        <div class=" justify-content-center">
            <div class="row p-1">
                <div class="col-xl-2 col-md-3 col-xs-12">
                    Username
                </div>
                <div class="col-xl-3 col-md-4 col-xs-12">
                    <input type="text" class="form-control custom-font-size" name="username" id="username"
                        placeholder="Enter username name">
                </div>
            </div>
            <div class="row p-1">
                <div class="col-xl-2 col-md-3 col-xs-12">
                    Roll number
                </div>
                <div class="col-xl-3 col-md-4 col-xs-12">
                    <input type="text" class="form-control custom-font-size" name="roll_no" id="roll_no"
                        placeholder="Enter Your Roll number">
                </div>
            </div>
            <div class="row p-1">
                <div class="col-xl-2 col-md-3 col-xs-12">
                    Age
                </div>
                <div class="col-xl-3 col-md-4 col-xs-12">
                    <input type="number" class="form-control custom-font-size" name="festival_name" id="festival_name"
                        placeholder="Enter Your Age">
                </div>
            </div>
            <div class="row p-1">
                <div class="col-xl-2 col-md-3 col-xs-12">
                    Email id
                </div>
                <div class="col-xl-3 col-md-4 col-xs-12">
                    <input type="email" class="form-control custom-font-size" name="email_id" id="email_id"
                        placeholder="Enter Your Roll number">
                </div>
            </div>
            <div class="row p-1">
                <div class="col-xl-2 col-md-3 col-xs-12">
                    Passsword
                </div>
                <div class="col-xl-3 col-md-4 col-xs-12">
                    <input type="password" class="form-control custom-font-size" name="password" id="password"
                        placeholder="Enter Your Passsword">
                </div>
            </div>
        </div>
        <div class="text-center">
            <button onclick="CreateUser()" class="btn btn-dark text-light">Sign Up</button>
        </div>

    </div>
    <script src="/scripts/signup.js"></script>
    </body>

    </html>